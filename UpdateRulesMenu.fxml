<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="570.0" stylesheets="@interfacemenu.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainJavaFX.UpdateRulesMenuController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="570.0" pickOnBounds="true">
         <image>
            <Image url="@background.jpg" />
         </image>
      </ImageView>
      <Label layoutX="202.0" layoutY="27.0" text="Update/Reset Rules" textFill="WHITE">
         <font>
            <Font name="Segoe UI Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="33.0" layoutY="139.0" text="Current Directory" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Button layoutX="137.0" layoutY="439.0" mnemonicParsing="false" onAction="#selectDir" text="Select Directory" />
      <Button layoutX="472.0" layoutY="491.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="49.0" text="Back" />
      <TextArea fx:id="directoryTextArea" editable="false" layoutX="170.0" layoutY="119.0" prefHeight="59.0" prefWidth="326.0" />
      <Button layoutX="333.0" layoutY="439.0" mnemonicParsing="false" onAction="#updateRules" text="Process Rules" />
      <Label layoutX="68.0" layoutY="265.0" text="Status" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font></Label>
      <TextArea fx:id="statusTextArea" editable="false" layoutX="170.0" layoutY="202.0" prefHeight="155.0" prefWidth="326.0" />
   </children>
</AnchorPane>
